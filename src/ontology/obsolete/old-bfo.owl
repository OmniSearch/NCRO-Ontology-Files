    <!-- http://purl.obolibrary.org/obo/snap_RealizableEntity -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/snap_RealizableEntity">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">realizable_entity</rdfs:label>
