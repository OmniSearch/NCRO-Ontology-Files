        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the east side of Saarbruecken</obo:IAO_0000112>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the lower right portion of a human torso</obo:IAO_0000112>
