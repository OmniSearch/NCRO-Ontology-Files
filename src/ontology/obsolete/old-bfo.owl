        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the disposition of blood to coagulate</obo:IAO_0000112>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the disposition of metal to conduct electricity</obo:IAO_0000112>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the function of the reproductive organs</obo:IAO_0000112>
        <obo:IAO_0000112 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">the role of being a doctor</obo:IAO_0000112>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/snap_Role -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/snap_Role">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">role</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/snap_RealizableEntity"/>
